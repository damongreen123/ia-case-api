{
  "provider": {
    "name": "am_roleAssignment_queryAssignment"
  },
  "consumer": {
    "name": "ia_caseApi"
  },
  "interactions": [
    {
      "description": "A query request for roles",
      "request": {
        "method": "POST",
        "path": "/am/role-assignments/query",
        "headers": {
          "Authorization": "Bearer someAuthorizationToken",
          "ServiceAuthorization": "someServiceAuthorizationToken"
        },
        "body": {
          "roleType": [
            "ORGANISATION"
          ],
          "roleName": [
            "tribunal-caseworker",
            "senior-tribunal-caseworker"
          ],
          "classification": [
            "PUBLIC",
            "RESTRICTED",
            "PRIVATE"
          ],
          "grantType": [
            "STANDARD"
          ],
          "validAt": "2021-12-04T00:00:00",
          "attributes": {
            "primaryLocation": [
              "500A2S"
            ],
            "jurisdiction": [
              "IA"
            ]
          }
        },
        "matchingRules": {
          "header": {
            "Authorization": {
              "matchers": [
                {
                  "match": "regex",
                  "regex": "Bearer someAuthorizationToken"
                }
              ],
              "combine": "AND"
            },
            "ServiceAuthorization": {
              "matchers": [
                {
                  "match": "regex",
                  "regex": "someServiceAuthorizationToken"
                }
              ],
              "combine": "AND"
            }
          }
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/vnd.uk.gov.hmcts.role-assignment-service.post-assignment-query-request+json;charset=UTF-8;version=1.0"
        },
        "body": {
          "roleAssignmentResponse": [
            {
              "actorId": "14a21569-eb80-4681-b62c-6ae2ed069e5f",
              "roleName": "senior-tribunal-caseworker",
              "roleCategory": "LEGAL_OPERATIONS",
              "readOnly": false,
              "attributes": {
                "primaryLocation": "500A2S",
                "jurisdiction": "IA"
              },
              "id": "14a21569-eb80-4681-b62c-6ae2ed069e6f",
              "roleType": "ORGANISATION",
              "classification": "PRIVATE",
              "grantType": "STANDARD",
              "actorIdType": "IDAM"
            }
          ]
        },
        "matchingRules": {
          "body": {
            "$.roleAssignmentResponse": {
              "matchers": [
                {
                  "match": "type",
                  "min": 1
                }
              ],
              "combine": "AND"
            },
            "$.roleAssignmentResponse[*].id": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            },
            "$.roleAssignmentResponse[*].attributes.jurisdiction": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            },
            "$.roleAssignmentResponse[*].attributes.primaryLocation": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            }
          }
        }
      },
      "providerStates": [
        {
          "name": "A list of role assignments for the search query"
        }
      ]
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "3.0.0"
    },
    "pact-jvm": {
      "version": "4.1.7"
    }
  }
}
