{
  "provider": {
    "name": "referenceData_professionalExternalUsers"
  },
  "consumer": {
    "name": "ia_caseApi"
  },
  "interactions": [
    {
      "description": "A Request to get users for an active organisation",
      "request": {
        "method": "GET",
        "path": "/refdata/external/v1/organisations/users",
        "headers": {
          "Authorization": "Bearer some-access-token",
          "ServiceAuthorization": "someServiceAuthToken"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=UTF-8"
        },
        "body": {
          "users": [
            {
              "userIdentifier": "userId"
            }
          ]
        },
        "matchingRules": {
          "body": {
            "$.users[0].userIdentifier": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            }
          },
          "header": {
            "Content-Type": {
              "matchers": [
                {
                  "match": "regex",
                  "regex": "application/json(;\\s?charset=[\\w\\-]+)?"
                }
              ],
              "combine": "AND"
            }
          }
        }
      },
      "providerStates": [
        {
          "name": "Professional users exist for an Active organisation"
        }
      ]
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "3.0.0"
    },
    "pact-jvm": {
      "version": "4.1.7"
    }
  }
}
